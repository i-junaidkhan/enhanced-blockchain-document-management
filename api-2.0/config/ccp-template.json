{
    "name": "logistics-network-${ORG_NAME}",
    "version": "1.0.0",
    "client": {
        "organization": "${ORG_MSP}",
        "connection": {
            "timeout": {
                "peer": {
                    "endorser": "300"
                }
            }
        }
    },
    "organizations": {
        "${ORG_MSP}": {
            "mspid": "${ORG_MSP}",
            "peers": [
                "${PEER_HOST}"
            ],
            "certificateAuthorities": [
                "${CA_HOST}"
            ]
        }
    },
    "peers": {
        "${PEER_HOST}": {
            "url": "grpcs://localhost:${PEER_PORT}",
            "tlsCACerts": {
                "pem": "${PEERPEM}"
            },
            "grpcOptions": {
                "ssl-target-name-override": "${PEER_HOST}"
            }
        }
    },
    "certificateAuthorities": {
        "${CA_HOST}": {
            "url": "https://localhost:${CA_PORT}",
            "caName": "${CA_NAME}",
            "tlsCACerts": {
                "pem": "${CAPEM}"
            },
            "httpOptions": {
                "verify": false
            }
        }
    }
}
